<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/head" />

    <com.baidu.mapapi.map.MapView
        android:id="@+id/map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/head_lin"></com.baidu.mapapi.map.MapView>
   <RelativeLayout
       android:id="@+id/share_rel"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/head_lin"
       android:visibility="gone"
       >
       <include layout="@layout/share_dialog"/>
   </RelativeLayout>
    <LinearLayout
        android:id="@+id/three_lin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="6dp"
        android:orientation="vertical"
        android:paddingBottom="160dp">

        <LinearLayout
            android:id="@+id/navigate_lin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/borderstyle_whiter"
            android:paddingBottom="12dp"
            android:paddingLeft="9dp"
            android:paddingRight="9dp"
            android:paddingTop="12dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/navigate" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="@drawable/borderstyle_whiter"
            android:orientation="vertical"
            android:paddingBottom="12dp"
            android:paddingLeft="9dp"
            android:paddingRight="9dp"
            android:paddingTop="12dp"
            android:visibility="gone"
            >

            <ImageView
                android:id="@+id/add_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/fangda" />

            <ImageView
                android:id="@+id/less_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:src="@mipmap/suoxao" />
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="75dp"
        android:layout_height="match_parent"
        android:layout_below="@+id/head_lin"
        android:layout_marginTop="300dp"
        >
        <LinearLayout
            android:id="@+id/year_lin1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/transparent_bg"
            android:orientation="vertical"
            android:paddingLeft="6dp"
            android:paddingRight="6dp"
            >

        <ImageView
            android:id="@+id/year_img1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="4dp"
            android:paddingTop="4dp"
            android:src="@mipmap/year"
            />
            <RelativeLayout
                android:id="@+id/year_rel1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:visibility="gone"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/yuan"
                    />
                <TextView
                    android:id="@+id/year_txt1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2017"
                    android:textSize="13sp"
                    android:textColor="@color/white"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/year_lin2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/transparent_bg1"
            android:orientation="vertical"
            android:gravity="right"
            android:visibility="gone"
            android:paddingRight="5dp"
            >
            <ImageView
                android:id="@+id/year_img2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="4dp"
                android:paddingTop="4dp"
                android:src="@mipmap/year"
                />
            <RelativeLayout
                android:id="@+id/year_rel2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:visibility="gone"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/yuan"
                    />
                <TextView
                    android:id="@+id/year_txt2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2017"
                    android:textSize="13sp"
                    android:textColor="@color/white"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <View
                android:layout_width="62dp"
                android:layout_height="2dp"
                android:background="@color/color_dddddd"
                android:layout_gravity="center"
                />
             <ListView
                 android:id="@+id/year_list"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:divider="@null"
                 >
             </ListView>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>